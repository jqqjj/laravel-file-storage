{
    "name": "jqqjj/laravel-file-storage",
    "type": "library",
    "description": "File storage for laravel",
    "keywords": ["laravel", "file", "storage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jqqjj",
            "email": "jqqjj@qq.com"
        }
    ],
    "require": {
        "php": "~7.0|~8.0",
        "illuminate/support": "~5.5|~6.0|~7.0|~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Jqqjj\\LaravelFileStorage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jqqjj\\LaravelFileStorage\\FileStorageServiceProvider"
            ],
            "aliases": {
                "FileStorage": "Jqqjj\\LaravelFileStorage\\Facades\\FileStorage"
            }
        }
    }
}
